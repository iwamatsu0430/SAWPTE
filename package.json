{
  "name": "SAWPTE",
  "version": "1.0.0",
  "description": "Presentation Timer Tools",
  "main": "main.js",
  "config": {
    "css_dir": "app/assets/stylesheets"
  },
  "scripts": {
    "start": "electron main.js",
    "sass": "node-sass --include-path scss $npm_package_config_css_dir/main.scss $npm_package_config_css_dir/main.css",
    "watch-sass": "watch 'npm run sass' $npm_package_config_css_dir",
    "watch": "npm run watch-sass"
  },
  "keywords": [
    "SAWPTE",
    "Presentation Timer"
  ],
  "author": "iwamatsu0430",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron-prebuilt": "^1.0.1",
    "node-sass": "^3.7.0",
    "watch": "^0.18.0"
  }
}
